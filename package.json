  },
  "keywords": [
    "babel",
    "stdin"
  ],
  "dependencies": {
    "babel-core": "^6.14.0",
    "find-up": "^1.1.2",
    "get-stdin": "^5.0.1"
  },
  "devDependencies": {
    "marked-man": "^0.1.5"
  }
